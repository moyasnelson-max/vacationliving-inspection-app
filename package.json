    "react-dom": "18.2.0",
    "@supabase/supabase-js": "2.38.4",
    "dotenv": "16.3.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.3",
    "lint-staged": "^15.2.0",
    "husky": "^8.0.0"
  }
